    "redux-logger": "^3.0.6",
    "redux-slider-monitor": "^2.0.0-0",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.17.0",
    "uglify-js": "git://github.com/mishoo/UglifyJS2#harmony-v2.8.22",
